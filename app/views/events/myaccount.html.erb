<div class="twelve columns white_bg ">

<h3>MY ACCOUNT</h3>

<ul class="tabs">
  <!-- Give href an ID value of corresponding "tabs-content" <li>'s -->
  <li><a class="active" href="#new">Active Events</a></li>
  <li><a href="#past">Past Events</a></li>
  <li><a href="#Going">Going</a></li>
</ul>


<ul class="tabs-content">
	<li class="active" id="new">
		<% @MyEvent.each do |myevent|%>
		
		<div class="three columns event_div" >
			<%= myevent.event_date.strftime("%B %d %Y") %>
		</div>
		<div class="five columns event_div">
			<%= link_to myevent.name, myevent%>  
		</div>
		<div class="three columns event_div">
			<%= link_to 'Edit', edit_event_path(myevent) %> 
		</div>
		<% end %>
			<div class=" pag_dev columns digg_pagination"> 
		<%= will_paginate @MyEvent %>
		</div>
	
	
	</li>
	<li  id="past">this is Past Events</li>
</ul>
</div>
